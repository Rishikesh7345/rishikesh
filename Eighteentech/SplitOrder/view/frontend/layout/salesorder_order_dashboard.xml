<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd"> 
	<update handle="customer_account"/>
    <head>
        <css src="Eighteentech_SplitOrder::css/backend.css"/>
    </head>
	<body> 
		<referenceBlock name="page.main.title"> 
			<action method="setPageTitle"> 
				<argument translate="true" name="title" xsi:type="string">Orders Manager</argument> 
			</action> 
		</referenceBlock> 
		<referenceContainer name="content"> 
			<block class="Eighteentech\SplitOrder\Block\Dashboard" name="order_dashboard" template="Eighteentech_SplitOrder::order/dashboard.phtml" cacheable="false" />		            
		</referenceContainer> 
	</body> 
	
	
	
	
</page>
